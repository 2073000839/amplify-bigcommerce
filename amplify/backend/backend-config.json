{
    "storage": {
        "DynamoStores": {
            "service": "DynamoDB",
            "providerPlugin": "awscloudformation"
        }
    },
    "function": {
        "CognitoStoresCreateAuthChallenge": {
            "service": "Lambda",
            "providerPlugin": "awscloudformation",
            "build": true
        },
        "CognitoStoresDefineAuthChallenge": {
            "service": "Lambda",
            "providerPlugin": "awscloudformation",
            "build": true
        },
        "CognitoStoresPreSignup": {
            "service": "Lambda",
            "providerPlugin": "awscloudformation",
            "build": true,
            "dependsOn": [
                {
                    "category": "storage",
                    "resourceName": "DynamoStores",
                    "attributes": [
                        "Name",
                        "Arn"
                    ]
                }
            ]
        },
        "CognitoStoresVerifyAuthChallengeResponse": {
            "service": "Lambda",
            "providerPlugin": "awscloudformation",
            "build": true
        },
        "bcApiClient": {
            "service": "Lambda",
            "providerPlugin": "awscloudformation",
            "build": true
        },
        "bcApiLambda": {
            "service": "Lambda",
            "providerPlugin": "awscloudformation",
            "build": true,
            "dependsOn": [
                {
                    "category": "storage",
                    "resourceName": "DynamoStores",
                    "attributes": [
                        "Name",
                        "Arn"
                    ]
                },
                {
                    "category": "function",
                    "resourceName": "bcApiClient",
                    "attributes": [
                        "Name"
                    ]
                },
                {
                    "category": "auth",
                    "resourceName": "CognitoStores",
                    "attributes": [
                        "UserPoolId"
                    ]
                }
            ]
        }
    },
    "auth": {
        "CognitoStores": {
            "service": "Cognito",
            "providerPlugin": "awscloudformation",
            "dependsOn": [
                {
                    "category": "function",
                    "resourceName": "CognitoStoresCreateAuthChallenge",
                    "triggerProvider": "Cognito",
                    "attributes": [
                        "Arn",
                        "Name"
                    ]
                },
                {
                    "category": "function",
                    "resourceName": "CognitoStoresDefineAuthChallenge",
                    "triggerProvider": "Cognito",
                    "attributes": [
                        "Arn",
                        "Name"
                    ]
                },
                {
                    "category": "function",
                    "resourceName": "CognitoStoresPreSignup",
                    "triggerProvider": "Cognito",
                    "attributes": [
                        "Arn",
                        "Name"
                    ]
                },
                {
                    "category": "function",
                    "resourceName": "CognitoStoresVerifyAuthChallengeResponse",
                    "triggerProvider": "Cognito",
                    "attributes": [
                        "Arn",
                        "Name"
                    ]
                }
            ]
        }
    },
    "api": {
        "bcApi": {
            "service": "API Gateway",
            "providerPlugin": "awscloudformation",
            "dependsOn": [
                {
                    "category": "function",
                    "resourceName": "bcApiLambda",
                    "attributes": [
                        "Name",
                        "Arn"
                    ]
                }
            ]
        }
    }
}